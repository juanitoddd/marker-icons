{
  "name": "marker-icons",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build marker-icons",
    "build:prod": "ng build marker-icons -c production",
    "build:full": "yarn generate-icons && yarn build:prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "rename-svgs": "node rename.js projects/marker-icons/icons/svg-icons '-15' ''",
    "generate-icons": "ng run marker-icons:generate-icons",
    "analyze": "ng build marker-icons-showcase --stats-json && webpack-bundle-analyzer ./dist/marker-icons-showcase/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.2",
    "@angular/common": "^19.1.2",
    "@angular/compiler": "^19.1.2",
    "@angular/core": "^19.1.2",
    "@angular/forms": "^19.1.2",
    "@angular/platform-browser": "^19.1.2",
    "@angular/platform-browser-dynamic": "^19.1.2",
    "@angular/router": "^19.1.2",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.3",
    "@angular-extensions/svg-icons-builder": "11.0.0",
    "@angular/cli": "^19.1.3",
    "@angular/compiler-cli": "^19.1.2",
    "@angular/language-service": "^19.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^22.0.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^19.1.0",
    "protractor": "~7.0.0",
    "svg-to-ts": "^12.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~5.5.4",
    "webpack-bundle-analyzer": "^3.6.1"
  }
}