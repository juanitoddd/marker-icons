{
  "name": "marker-icons",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build marker-icons",
    "build:prod": "ng build marker-icons -c production",
    "build:full": "yarn generate-icons && yarn build:prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "rename-svgs": "node rename.js projects/marker-icons/icons/svg-icons '-15' ''",
    "generate-icons": "svg-to-ts",
    "analyze": "ng build marker-icons-showcase --stats-json && webpack-bundle-analyzer ./dist/marker-icons-showcase/stats-es2015.json"
  },
  "svg-to-ts": {
    "srcFiles": [
      "./projects/marker-icons/icons/svg-icons-new/**/*.svg"
    ],
    "outputDirectory": "./projects/marker-icons/src/icons",
    "interfaceName": "MarkerIcon",
    "typeName": "markerIcon",
    "prefix": "marker",
    "fileName": "marker-icons",
    "optimizeForLazyLoading": false,
    "modelFileName": "marker-icon.model",
    "compileSources": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.2",
    "@angular/common": "^17.2.2",
    "@angular/compiler": "^17.2.2",
    "@angular/core": "^17.2.2",
    "@angular/forms": "^17.2.2",
    "@angular/platform-browser": "^17.2.2",
    "@angular/platform-browser-dynamic": "^17.2.2",
    "@angular/router": "^17.2.2",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.1",
    "@angular/cli": "^17.2.1",
    "@angular/compiler-cli": "^17.2.2",
    "@angular/language-service": "^17.2.2",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^17.2.0",
    "protractor": "~7.0.0",
    "svg-to-ts": "^4.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~5.3.3",
    "webpack-bundle-analyzer": "^3.6.1"
  }
}